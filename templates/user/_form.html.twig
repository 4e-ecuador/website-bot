{{ form_start(form) }}

<div class="row">
    <div class="col-sm">
        {{ form_row(form.email) }}
    </div>
    <div class="col-sm">
        {{ form_row(form.googleId) }}
    </div>
</div>
<div class="row">
    <div class="col-sm">
        {{ form_row(form.agent) }}
    </div>
    <div class="col-sm">
        {{ form_row(form.roles) }}
    </div>
</div>

{{ form_rest(form) }}

<button class="btn btn-outline-success">{{ button_label|default('Save') }}</button>

{{ form_end(form) }}
