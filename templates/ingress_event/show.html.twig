{% extends 'base.html.twig' %}

{% block title %}IngressEvent{% endblock %}

{% block body %}
    <h1>IngressEvent</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ ingress_event.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ ingress_event.name }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ ingress_event.type }}</td>
            </tr>
            <tr>
                <th>Date_start</th>
                <td>{{ ingress_event.dateStart ? ingress_event.dateStart|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Date_end</th>
                <td>{{ ingress_event.dateEnd ? ingress_event.dateEnd|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ ingress_event.description }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('ingress_event_index') }}">back to list</a>

    <a href="{{ path('ingress_event_edit', {'id': ingress_event.id}) }}">edit</a>

    {{ include('ingress_event/_delete_form.html.twig') }}
{% endblock %}
