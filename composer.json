{
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^7.4",
    "ext-ctype": "*",
    "ext-curl": "*",
    "ext-dom": "*",
    "ext-gd": "*",
    "ext-iconv": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "api-platform/api-pack": "^1.2",
    "boshurik/telegram-bot-bundle": "^4.0",
    "doctrine/doctrine-fixtures-bundle": "^3.2",
    "fabpot/goutte": "^4.0",
    "google/apiclient": "^2.5",
    "knplabs/knp-markdown-bundle": "^1.7",
    "knpuniversity/oauth2-client-bundle": "^2.0",
    "league/oauth2-google": "^3.0",
    "nelmio/cors-bundle": "^2.0",
    "sensio/framework-extra-bundle": "^5.1",
    "symfony/apache-pack": "^1.0",
    "symfony/asset": "5.*",
    "symfony/console": "5.*",
    "symfony/dotenv": "5.*",
    "symfony/expression-language": "5.*",
    "symfony/flex": "^1.1",
    "symfony/form": "5.*",
    "symfony/framework-bundle": "5.*",
    "symfony/google-mailer": "5.*",
    "symfony/http-client": "5.*",
    "symfony/intl": "5.*",
    "symfony/mailer": "5.*",
    "symfony/monolog-bundle": "^3.1",
    "symfony/orm-pack": "*",
    "symfony/process": "5.*",
    "symfony/security-bundle": "5.*",
    "symfony/serializer-pack": "*",
    "symfony/translation": "5.*",
    "symfony/twig-pack": "^1.0",
    "symfony/validator": "5.*",
    "symfony/web-link": "5.*",
    "symfony/webpack-encore-bundle": "^1.6",
    "symfony/yaml": "5.*",
    "twig/cssinliner-extra": "^3.0",
    "twig/inky-extra": "^3.0",
    "twig/markdown-extra": "^3.0",
    "twig/string-extra": "^3.0"
  },
  "require-dev": {
    "hautelook/alice-bundle": "^2.7",
    "roave/security-advisories": "dev-master",
    "symfony/debug-pack": "*",
    "symfony/maker-bundle": "^1.0",
    "symfony/profiler-pack": "*",
    "symfony/test-pack": "*"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "translate": [
      "@translate-all"
    ],
    "translate-all": {
      "en": "./bin/console translation:update --output-format=yaml --force en",
      "es": "./bin/console translation:update --output-format=yaml --force es",
      "de": "./bin/console translation:update --output-format=yaml --force de"
    }
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "5.*"
    }
  }
}
