{
  "name": "4e-ecuador/website-bot",
  "description": "A web site and a bot.",
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": ">=8.1",
    "ext-ctype": "*",
    "ext-curl": "*",
    "ext-dom": "*",
    "ext-gd": "*",
    "ext-iconv": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "boshurik/telegram-bot-bundle": "5.0.0",
    "composer/package-versions-deprecated": "1.11.99.3",
    "doctrine/annotations": "^1.0",
    "doctrine/doctrine-bundle": "^2.4",
    "doctrine/doctrine-fixtures-bundle": "^3.2",
    "doctrine/doctrine-migrations-bundle": "^3.1",
    "doctrine/orm": "^2.9",
    "fabpot/goutte": "^4.0",
    "knplabs/knp-time-bundle": "^1.19",
    "knpuniversity/oauth2-client-bundle": "^2.0",
    "league/csv": "^9.8",
    "league/oauth2-google": "^4.0",
    "michelf/php-markdown": "^2.0",
    "nelmio/cors-bundle": "^2.0",
    "phpdocumentor/reflection-docblock": "^5.2",
    "symfony/apache-pack": "^1.0",
    "symfony/asset": "6.*",
    "symfony/console": "6.*",
    "symfony/dotenv": "6.*",
    "symfony/expression-language": "6.*",
    "symfony/flex": "^2.1",
    "symfony/form": "6.*",
    "symfony/framework-bundle": "6.*",
    "symfony/google-mailer": "6.*",
    "symfony/http-client": "6.*",
    "symfony/intl": "6.*",
    "symfony/mailer": "6.*",
    "symfony/monolog-bundle": "^3.1",
    "symfony/process": "6.*",
    "symfony/property-access": "6.*",
    "symfony/property-info": "6.*",
    "symfony/proxy-manager-bridge": "6.*",
    "symfony/runtime": "6.*",
    "symfony/security-bundle": "6.*",
    "symfony/serializer": "6.*",
    "symfony/translation": "6.*",
    "symfony/twig-bundle": "6.*",
    "symfony/validator": "6.*",
    "symfony/web-link": "6.*",
    "symfony/webpack-encore-bundle": "^1.6",
    "symfony/yaml": "6.*",
    "tattali/calendar-bundle": "^1.2",
    "twig/cssinliner-extra": "^3.0",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/inky-extra": "^3.0",
    "twig/string-extra": "^3.0",
    "twig/twig": "^2.12|^3.0"
  },
  "require-dev": {
    "elkuku/symfony-utils": "dev-master",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan": "^1.4",
    "phpstan/phpstan-doctrine": "^1.2",
    "phpstan/phpstan-symfony": "^1.1",
    "phpunit/phpunit": "^9.5",
    "rector/rector": "^0.15",
    "roave/security-advisories": "dev-latest",
    "symfony/browser-kit": "6.*",
    "symfony/css-selector": "6.*",
    "symfony/debug-bundle": "6.*",
    "symfony/maker-bundle": "^1.0",
    "symfony/phpunit-bridge": "6.*",
    "symfony/stopwatch": "6.*",
    "symfony/web-profiler-bundle": "6.*"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "symfony/flex": true,
      "symfony/runtime": true,
      "phpstan/extension-installer": true
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "translate": [
      "@translate-all"
    ],
    "translate-all": {
      "en": "./bin/console translation:extract --format=yaml --force en --domain=messages",
      "es": "./bin/console translation:extract --format=yaml --force es --domain=messages",
      "de": "./bin/console translation:extract --format=yaml --force de --domain=messages"
    },
    "test-phpunit": ["make tests"],
    "test-phpstan": ["vendor/bin/phpstan analyse"],
    "ci": ["@test-phpunit", "@test-phpstan"]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "6.*",
      "docker": true
    }
  }
}
